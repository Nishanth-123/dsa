class Solution {
public:
    int singleNumber(vector<int>& arr) {
      int res=0;
        
      for(int i=0;i<32;i++){
          int x=((1<<i) | (0b00000000000000000000000000000000));
          int sum=0;
          for(int j=0;j<arr.size();j++){
              if(arr[j]& x) sum++;
          }
          if(sum%3)res+=x;
      } 
        return res;
    }
};
