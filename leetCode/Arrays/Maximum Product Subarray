#define ll int
class Solution {
public:
    int maxProduct(vector<int>& arr) {
        ll ans=INT_MIN;
        ll maxProd=1, minProd=1;
        ll n=arr.size();
        for(ll i=0;i<n;i++){
            if(arr[i]<0)swap(minProd, maxProd);
            maxProd=max(maxProd*arr[i], arr[i]);
            minProd=min(minProd*arr[i], arr[i]);
            ans=max(ans, maxProd);
        }
        return ans;
    }
};
